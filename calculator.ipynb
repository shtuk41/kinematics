   "source": [
    "import numpy as np\n",
    "\n",
    "V = np.array([2,2])\n",
    "X = np.array([1,0])\n",
    "\n",
    "V_norm = V / np.linalg.norm(V)\n",
    "\n",
    "cosQ = V_norm.dot(X)\n",
    "Q = np.rad2deg(np.arccos(cosQ))\n",
    "\n",
    "print(Q)"
   ]
  }
